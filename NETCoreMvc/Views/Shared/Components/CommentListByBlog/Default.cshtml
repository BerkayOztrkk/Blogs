@model List<EntityLayer.Entities.Comment>



	<div class="comment-top">
		<h4>Comments</h4>
		@foreach (var item in Model)
		{
		<h5 class="mt-0">@item.Username</h5>
		<div class="media">
			
			<img src="~/web/images/t1.jpg" alt="" class="img-fluid" />
			<div class="media-body">
				<h5 class="mt-0"></h5>
				<p>
					@item.Content
				</p>

				
			</div>
		
		</div>
	}
	</div>
